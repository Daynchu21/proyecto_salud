{
  "expo": {
    "name": "proyectosaludapp",
    "slug": "proyectosaludapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/icon_1024.png",
    "scheme": "proyectosaludapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSMicrophoneUsageDescription": "Necesitamos acceder al micrófono para grabar mensajes de voz."
      },
      "bundleIdentifier": "com.daynchu20.proyectosaludapp"
    },
    "android": {
      "icon": "./assets/icons/icon_1024.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/icon_1024.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "googleServicesFile": "./google-services.json",
      "permissions": [
        "RECORD_AUDIO",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ],
      "googleMaps": {
        "apiKey": "AIzaSyDWnHTJmR22Z7tQGDg46tr6fyuZWkgNsMo"
      },
      "package": "com.daynchu20.proyectosaludapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/icons/favicon_48.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-notifications",
        {
          "icon": "./assets/notification-icon.png",
          "color": "#ffffff",
          "sounds": ["./assets/alerta.mp3"]
        }
      ],
      [
        "expo-audio",
        {
          "microphonePermission": "Permitir que $(PRODUCT_NAME) acceda al micrófono."
        }
      ]
    ],
    "assetBundlePatterns": ["**/*", "assets/alerta.mp3"],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "50525bba-4d50-4097-b119-be4f6112bc17"
      },
      "apiStatusUrl": "https://back-status-salud.fly.dev",
      "analyticsEnabled": false
    },
    "owner": "daynchu20"
  }
}
